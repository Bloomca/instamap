{
  "parser": "babel-eslint",
  "ecmaFeatures": {
      "classes": true,
      "jsx": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [0],
    "react/prefer-stateless-function": [1],

    "import/prefer-default-export": [0],

    "no-underscore-dangle": [0],
    "no-return-assign": [0]
  }
}
